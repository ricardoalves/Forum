<h2>Sign in</h2>
<%= form_for @user, url: sessions_url do |f| %>
	<%= f.error_messages %>
	<p>
		<%= f.label :email %>
		<%= f.email_field :email %>
	</p>

	<p>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</p>

	<p>
		<%= f.submit "Sign in" %>
	</p>
<% end %>

<%= link_to "New User?", user_sign_up_url %>